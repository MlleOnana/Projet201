<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.applicationnote.ConsultationListeUserController">
   <children>
      <Pane prefHeight="500.0" prefWidth="800.0">
         <children>
            <Button fx:id="ajout" layoutX="715.0" layoutY="70.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="25.0" prefWidth="71.0" text="REFRESH" />
            <TableView fx:id="professeur" layoutY="125.0" prefHeight="375.0" prefWidth="800.0">
              <columns>
                <TableColumn fx:id="nomP" prefWidth="271.0" text="NOM ET PRENOM" />
                <TableColumn fx:id="telP" prefWidth="136.0" text="TELEPHONE" />
                  <TableColumn fx:id="emailP" prefWidth="168.0" text="EMAIL" />
                  <TableColumn fx:id="matiereP" prefWidth="224.0" text="MATIERES ENSEIGNEES" />
              </columns>
            </TableView>
            <ImageView fitHeight="40.0" fitWidth="35.0" layoutX="32.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../arrow-left.png" />
               </image>
            </ImageView>
            <Button fx:id="supprimer" layoutX="602.0" layoutY="70.0" mnemonicParsing="false" onAction="#suppression" prefHeight="25.0" prefWidth="86.0" text="SUPPRIMER" />
            <Label fx:id="erreurListe" layoutX="159.0" layoutY="70.0" prefHeight="34.0" prefWidth="394.0" textFill="RED">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="retourB" layoutX="21.0" layoutY="20.0" mnemonicParsing="false" onAction="#retour" opacity="0.44" prefHeight="50.0" prefWidth="58.0" style="-fx-border-color: grey; -fx-border-width: 2px;" />
         </children>
      </Pane>
   </children>
</AnchorPane>
